       (Terminal
            (newclass Terminal Expression (char) ()
                ((char (s) ((getself s) 'objectAt: 2))
                 (char: (s new) ((getself s) 'objectAt:put: 2 new))
                 (privateMatch:in: (s input scope)
                    (let* ((self (getself s))
                           (char (self 'char)))
                        (if (= (input 'next) char) char null)))
                )
                ((char: (s char)
                    (let ((result ((getself s) 'new)))
                        (result 'char: char)
                        result)))
            ))
